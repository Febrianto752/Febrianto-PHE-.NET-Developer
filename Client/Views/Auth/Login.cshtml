@model LoginVM


<div class="row justify-content-center">
    <div class="col-sm-8 ">
        <partial name="_FlashMessage" />
        <div class="card p-4 shadow-lg rounded">
            <div class="card-body">
                <h3 class="card-title mb-5 text-center">Masuk</h3>

                <form method="post" asp-action="Login" enctype="multipart/form-data">
                    <div class="mb-3">
                        <label asp-for="Email" class="form-label">Email address</label>
                        <input class="form-control" asp-for="Email" aria-describedby="emailHelp" required>
                    </div>

                    <div class="mb-3">
                        <label asp-for="Password" class="form-label">Password</label>
                        <input type="password" class="form-control" asp-for="Password" required>
                    </div>

                   
                    <button id="btnRegister" class="btn btn-primary mt-4">Masuk</button>
                </form>
            </div>
        </div>
    </div>
</div>